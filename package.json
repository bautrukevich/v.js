{
  "name": "v.js",
  "version": "0.5.0",
  "description": "Small validation library",
  "main": "dist/v.min.js",
  "scripts": {
    "build": "webpack -p --env=build",
    "dev": "webpack-dev-server --hot --open",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdocrc --verbose"
  },
  "repository": {
    "url": "https://github.com/bautrukevich/v.js.git",
    "type": "git"
  },
  "author": "Siarhei Bautrukevich",
  "license": "MIT",
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs",
    "validation"
  ],
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.10.3",
    "jsdoc": "3.6.7",
    "minami": "1.2.3",
    "mocha": "3.5.3",
    "webpack": "beta",
    "webpack-dev-server": "1.16.5",
    "yargs": "6.6.0"
  },
  "dependencies": {
    "libphonenumber-js": "^0.2.31"
  }
}
